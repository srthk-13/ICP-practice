class Solution {
    public int timeRequiredToBuy(int[] tickets, int k) {
        int tk=tickets[k];
        int time=0;
        int n=tickets.length;
        for(int i=0;i<n;i++){
            if(i<=k) time+=Math.min(tk,tickets[i]);
            else time+=Math.min(tk-1,tickets[i]);
        }
        return time;
    }
}
